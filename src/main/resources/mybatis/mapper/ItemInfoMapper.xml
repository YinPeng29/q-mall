<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.bays.dao.ItemMapper">

    <insert id="addItem">
        insert into iteminfo
        (id,item_number,logo,name,type,size,is_online,sex,description)
        values
        (#{item.id},#{item.number},#{item.logo},#{item.name},#{item.type},#{item.size},#{item.is_online},#{item.sex},#{item.description})
    </insert>
</mapper>